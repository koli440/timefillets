<Window x:Class="TimeFillets.MainApplication.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:TimeFillets.ViewModel;assembly=TimeFillets.ViewModel"
        Title="Progress" Height="48" Width="300" WindowStyle="SingleBorderWindow" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <StackPanel>
        <ProgressBar Name="WorkProgressBar" Value="{Binding Path=PercentCount}" Maximum="100" Height="20"></ProgressBar>
    </StackPanel>
</Window>
