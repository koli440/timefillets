<Window x:Class="TimeFillets.MainApplication.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ProgressWindow" Height="100" Width="300">
    <StackPanel>
        <ProgressBar Value="{Binding Path=PercentCompleted}" Maximum="100"></ProgressBar>
    </StackPanel>
</Window>
